{
  "name": "json-server-example",
  "description": "An example of json-server, load data from server by page",
  "version": "1.0.0",
  "author": "Helen Nikitina <amaterasu.san@gmail.com>",
  "scripts": {
    "watch": "webpack --watch",
    "build": "webpack",
    "json-server": "json-server ./src/static/test-data.json",
    "web-server": "webpack-dev-server --mode development --open",
    "start": "concurrently --kill-others \"npm run json-server\" \"npm run web-server\""
  },
  "keywords": [
    "json-server-example",
    "pagination"
  ],
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.2.0",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "json-server": "^0.17.0",
    "mini-css-extract-plugin": "^2.6.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  }
}
